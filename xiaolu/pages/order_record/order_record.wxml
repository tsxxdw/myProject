<view>

  <block wx:for='{{dataList}}' wx:key='list' wx:for-item="item">
    <view class="item">
      <view class="item_1" />
      <view class="item_2">
        <text style="font-size: 30rpx;float:left;margin-left:3%">
          搬运时间：{{item.carryDate}}
        </text>
       <text style="font-size: 26rpx;color:#717171;float:right;margin-right:3%;">
          交易中
        </text> 
      </view>
      <view class="item_3">

      </view>
      <view class="item_4">
        <view style="width:100%;height:20px;">
          <text style="left:3%;width:100%;height:20px;position: absolute;color:#BEBEBE;font-size: 26rpx">订单编号： {{item.id}}</text>
        </view>
        <view style="width:100%;height:30px;">
          <view style="left:3%;width:200px;height:20px;position: absolute;color:#BEBEBE;font-size: 26rpx">交易时间： {{item.createDate}}</view>
          <view style="right:6%;width:60px;height:20px;position: absolute;">￥ {{item.assessmentCosts}}</view>
        </view>
      </view>

      <view class="item_5">
             <view class="item_5_item1">信息补充</view>
      </view>

    </view>
  </block>
</view>
<view class="bottom_view">
 没有更多订单了~
</view>